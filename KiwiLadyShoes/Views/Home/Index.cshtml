@{
    ViewData["Title"] = "Home Page";
}


<!--Bootstrap slideshow, Ira, 29.09.23--------------------------------->
<div class="container">
    <div id="slideshow" class="carousel slide" data-bs-ride="carousel">
        <div class="carousel-inner">
            <div class="carousel-item active" data-bs-interval="2000">
                <img src="~/img/1_homePage.webp" class="d-block w-100" alt="Image1">
            </div>
            <div class="carousel-item" data-bs-interval="2000">
                <img src="~/img/2_homePage.webp" class="d-block w-100" alt="Image2">
            </div>
            <div class="carousel-item" data-bs-interval="2000">
                <img src="~/img/3_homePage.webp" class="d-block w-100" alt="Image3">
            </div>
            <div class="carousel-item" data-bs-interval="2000">
                <img src="~/img/4_homePage.webp" class="d-block w-100" alt="Image3">
            </div>
            <div class="carousel-item" data-bs-interval="2000">
                <img src="~/img/5_homePage.webp" class="d-block w-100" alt="Image3">
            </div>
        </div>
        <button class="carousel-control-prev" type="button" data-bs-target="#slideshow" data-bs-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Previous</span>
        </button>
        <button class="carousel-control-next" type="button" data-bs-target="#slideshow" data-bs-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
        </button>
    </div>
</div>
<!--End of Bootstrap animation, Ira, 29.09.23--------------------------------->
